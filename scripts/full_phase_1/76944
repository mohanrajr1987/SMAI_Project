ersion FROM methods WHERE id  OLD.id               1 WHERE ROWID  OLD.ROWID         INSERT INTO methods id,version,name VALUES OLD.id,OLD.version,OLD.name     END  INSERT INTO methods name VALUES Test INSERT INTO methods name VALUES Test2  UPDATE methods SET name  Tested WHERE id  1  SELECT  FROM methods  121Tested 211Test2 110Test   codepre
