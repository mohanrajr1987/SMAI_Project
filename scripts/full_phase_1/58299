lldate BETWEEN 20150504 000000 AND 20150504 235959                 UNION ALL                 SELECT                      callend as cd,                      1 as t                  FROM cdr c                 WHERE                      callend BETWEEN 20150504 000000 AND 20150504 235959                 ORDER BY cd              x          GROUP BY Interval      y codepre
