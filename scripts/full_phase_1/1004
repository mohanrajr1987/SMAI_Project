sum          INSERT INTO [dbo].[LTACTEST1]     SELECT DISTINCT ds.CLAIMID, ds.CIN, ds.SVCDATE,       ds.SVCDATETO, ds.totaldiff as TOTALDAYS, ds.chaincount as CHAINCOUNT     FROM diffsum ds     JOIN diffcomp dc     ON ds.chainID  dc.chainID AND ds.CIN  dc.CIN       AND ds.totaldiff  dc.totaldiff     OPTION maxrecursion 0 END GO codepre
