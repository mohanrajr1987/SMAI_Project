P 1     CASTM.MeterName as varcharMAX As MeterName,     CASTM.MID as varcharMAX As MID,     CASTD.DeviceType as varcharMAX As DeviceType   FROM Meter M     INNER JOIN CurrentDevice D ON       D.ClientNumber  M.ClientNumber AND       D.MID  M.MID   WHERE      M.ClientNumber  12300  T UNPIVOT[FieldValue] FOR FieldName IN [MeterName],[MID],[DeviceType] UP codepre
