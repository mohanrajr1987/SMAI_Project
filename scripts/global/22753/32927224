turn 1 } func tableView(tableView: UITableView, numberOfRowsInSection section: Int) -&gt; Int {     return 5 } func tableView(tableView: UITableView, cellForRowAtIndexPath indexPath: NSIndexPath) -&gt; UITableViewCell {     let cell:CustomTareasCell = tableView.dequeueReusableCellWithIdentifier(CellIdentifier, forIndexPath: indexPath) as! CustomTareasCell     cell.tareaLabel.text = tareas[indexPath.row];     cell.indiceLabel.text = String(indexPath.row+1);     return cell } </code></pre>