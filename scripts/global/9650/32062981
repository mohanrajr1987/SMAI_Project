   Dim rows = TableClassA12.Descendants("tr")     Dim header = rows.Skip(1).First()      For Each column In header.Descendants("td")         result.Columns.Add(New DataColumn(column.InnerText.Trim, GetType(String)))     Next      For Each row In rows.Skip(2)         Dim data = New List(Of String)()         For Each column In row.Descendants("td")             Dim cellText As String = column.InnerText.Trim             data.Add(cellText)         Next         If data.Count &gt; 0 Then             result.Rows.Add(data.ToArray())         End If     Next     Return result End Function </code></pre>