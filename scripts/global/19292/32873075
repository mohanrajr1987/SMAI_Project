<pre><code>void Print(Node *head) {      if (head = NULL)     {         cout &lt;&lt; "NULL";     }     else     {         cout &lt;&lt; head-&gt;data &lt;&lt; endl;         head = head-&gt;next;         while (head-&gt;next != NULL)         {             cout &lt;&lt; head-&gt;data &lt;&lt; endl;             head = head-&gt;next;         }         cout &lt;&lt; "NULL";     } } </code></pre>