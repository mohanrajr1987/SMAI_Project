<pre><code>#include &lt;stdio.h&gt; int main() {     int x,y,z;      printf ("Side x: ");     scanf ("%d",&amp;x);      printf ("Side y: ");     scanf ("%d",&amp;y);      printf ("Side z: ");     scanf ("%d",&amp;z);      if (x+y&gt;z&amp;&amp;x+z&gt;y&amp;&amp;z+y&gt;x)         printf ("\nTriangle can be performed");     else         printf ("\nTriangle cannot be performed");     return 0; } </code></pre>